<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.homeward.webstore.mapper.OrderMapper">
    <cache/>
    <!--<resultMap id="information" type="PlayerInfo" autoMapping="true">-->
    <resultMap id="items" type="ItemsList" autoMapping="true">
        <id column="id" property="id"/>
    </resultMap>
    <select id="getItemId" resultMap="items" useCache="true">
        select id from items
        <where>
            <if test="itemId != null and itemId != ''">
                id = #{itemId}
            </if>
        </where>
    </select>
</mapper>