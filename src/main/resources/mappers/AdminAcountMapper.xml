<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.homeward.webstore.mapper.AdminAccountMapper">
    <select id="AdministratorLogin" resultType="Long" useCache="true">
        select count(*)
        from admin_info
        where username = #{adminName} and password = #{adminPwd}
    </select>
</mapper>